<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Index</servlet-name>
		<servlet-class>galaxyoyo.web.neel.servlets.Index</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Register</servlet-name>
		<servlet-class>galaxyoyo.web.neel.servlets.Register</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>galaxyoyo.web.neel.servlets.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Vote</servlet-name>
		<servlet-class>galaxyoyo.web.neel.servlets.Vote</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Recruitment</servlet-name>
		<servlet-class>galaxyoyo.web.neel.servlets.Recrutement</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Robots</servlet-name>
		<servlet-class>galaxyoyo.web.neel.servlets.Robots</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Query</servlet-name>
		<servlet-class>galaxyoyo.web.neel.servlets.QueryTMP</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Index</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/inscription</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/connexion</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Vote</servlet-name>
		<url-pattern>/vote</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Recruitment</servlet-name>
		<url-pattern>/recrutement</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Robots</servlet-name>
		<url-pattern>/robots.txt</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Query</servlet-name>
		<url-pattern>/query</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>Objectify</filter-name>
		<filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
	</filter>
	<filter>
	    <filter-name>QueryFilter</filter-name>
	    <filter-class>galaxyoyo.web.neel.filters.QueryFilter</filter-class>
	</filter>
	<filter>
	    <filter-name>Login</filter-name>
	    <filter-class>galaxyoyo.web.neel.filters.Login</filter-class>
	</filter>
	<filter>
	    <filter-name>ExceptionCatcher</filter-name>
	    <filter-class>galaxyoyo.web.neel.filters.ExceptionCatcher</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>Objectify</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
	    <filter-name>QueryFilter</filter-name>
	    <url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
	    <filter-name>Login</filter-name>
	    <url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
	    <filter-name>ExceptionCatcher</filter-name>
	    <url-pattern>/*</url-pattern>
	</filter-mapping>
	<welcome-file-list>
		<welcome-file>index</welcome-file>
	</welcome-file-list>
</web-app>
